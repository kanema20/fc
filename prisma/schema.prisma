generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model EmailSignup {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  source    String   @default("cakeshop_landing")
  createdAt DateTime @default(now()) @map("created_at")

  @@map("email_signups")
}
